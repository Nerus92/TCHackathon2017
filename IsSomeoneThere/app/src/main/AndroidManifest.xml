<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ist.android.issomeonethere">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:name="com.ist.android.issomeonethere.MainApplication"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HelpCategoriesActivity"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MedicalSituationActivity"
            android:parentActivityName=".HelpCategoriesActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".IsRoadAccessibleActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".WhyNoRoadActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ChatActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".RescueActivity"
            android:parentActivityName=".HelpCategoriesActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SpecialNeedsActivity"
            android:parentActivityName=".RescueActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ConfirmLocationActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CanYouMoveActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MapActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CanYouGoThereActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".TransportationTypeActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".BoatTypeActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CarTypeActivity"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>